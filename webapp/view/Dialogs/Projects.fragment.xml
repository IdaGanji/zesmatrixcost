<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Select Project"
		search=".onProjectSearch"
		confirm=".onProjectValueHelpDialogClose"
		cancel=".onProjectValueHelpDialogClose"
        showClearButton="true"
        resizable="true"
        multiSelect="true"
		items="{
			path: 'ProjectValueHelpModel>/results'
		}">
		<StandardListItem
			title="{ProjectValueHelpModel>ESIProject}"
			type="Active"/>
	</SelectDialog>
</core:FragmentDefinition>